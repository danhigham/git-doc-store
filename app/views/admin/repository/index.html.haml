-if @repos.length == 0 
  %h3 No registered repositories
-else
  %table#admin-repo-list
    %tr
      %th.name Name
      %th Path
      %th.date Created @
      %th.unreg Unregister
    -@repos.each do |r|
      %tr
        %td= r.name
        %td= r.path
        %td= r.created_at
        %td= modal_link("Unregister repository", "x" , "/admin/repository/delete/#{r.name}")


